     <div class="container">
        <h1>Tela de Consulta</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>Nome</th>
                    <th>Endereço</th>
                    <th>Bairro</th>
                    <th>CEP</th>
                    <th>Cidade</th>
                    <th>Estado</th>
                    <th>Ações</th>
                </tr>
            </thead>
            <tbody>
                {{#each post}}
                {{#dataValues}}
                <tr>
                    <td>{{nome}}</td>
                    <td>{{endereco}}</td>
                    <td>{{bairro}}</td>
                    <td>{{cep}}</td>
                    <td>{{cidade}}</td>
                    <td>{{estado}}</td>
                    <td>
                        <a href="/editar/{{id}}"><button class="btn btn-primary">Editar</button></a>
                        <a href="/excluir/{{id}}"><button class="btn btn-danger">Excluir</button></a>
                    </td>
                </tr>
                {{/dataValues}}
                {{/each}}
            </tbody>
        </table>
    </div>